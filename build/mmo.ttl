@prefix : <http://purl.org/MMO#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://purl.org/MMO> .

<http://purl.org/MMO> rdf:type owl:Ontology ;
                       owl:versionIRI <http://purl.org/MMO/0.1> ;
                       <http://purl.org/dc/elements/1.1/contributor> "" ;
                       <http://purl.org/dc/elements/1.1/creator> <https://www.umr-lastig.fr/mattia-bunel/static/foaf.rdf#me> ;
                       <http://purl.org/dc/elements/1.1/description> "TODO : Description en français"@fr ,
                                                                     "TODO :Description in english"@en ;
                       <http://purl.org/dc/elements/1.1/issued> "Not yet" ;
                       <http://purl.org/dc/elements/1.1/licence> <http://cecill.info/licences/Licence_CeCILL_V2.1-en.html> ;
                       <http://purl.org/dc/elements/1.1/publisher> <https://anr.fr/Projet-ANR-19-CE38-0003> ;
                       <http://purl.org/dc/elements/1.1/rights> "ANR CORES 2021" ;
                       <http://purl.org/dc/elements/1.1/title> "Mental Map Ontology"@en ,
                                                               "Ontologie des cartes mentales"@fr ;
                       <http://purl.org/dc/terms/modified> "2021-04-19"^^xsd:string ;
                       <http://purl.org/vocab/vann/preferredNamespacePrefix> "mmo" ;
                       <http://purl.org/vocab/vann/preferredNamespaceUri> <http://purl.org/MMO> ;
                       rdfs:comment "**MMO** est l'ontologie des cartes mentales"^^xsd:string ;
                       rdfs:label "Label"^^xsd:string ;
                       owl:versionInfo "0.1"^^xsd:string .

#################################################################
