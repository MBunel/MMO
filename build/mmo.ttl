#################################################################

###  http://purl.org/dc/elements/1.1/contributor
<http://purl.org/dc/elements/1.1/contributor> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
<http://purl.org/dc/elements/1.1/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/issued
<http://purl.org/dc/elements/1.1/issued> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/licence
<http://purl.org/dc/elements/1.1/licence> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/publisher
<http://purl.org/dc/elements/1.1/publisher> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/rights
<http://purl.org/dc/elements/1.1/rights> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
<http://purl.org/vocab/vann/preferredNamespacePrefix> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
<http://purl.org/vocab/vann/preferredNamespaceUri> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#minQualifiedCardinality
owl:minQualifiedCardinality rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/MMO#AboutiÀ
:AboutiÀ rdf:type owl:ObjectProperty ;
         owl:equivalentProperty [ owl:inverseOf :IssuDe
                                ] ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         owl:inverseOf :IssuDe ;
         owl:propertyChainAxiom ( :EstUtiliséPourProduirePar
                                  :Produit
                                ) .


###  http://purl.org/MMO#Compose
:Compose rdf:type owl:ObjectProperty ;
         owl:equivalentProperty [ owl:inverseOf :EstComposéDe
                                ] ;
         owl:inverseOf :EstComposéDe ;
         rdf:type owl:AsymmetricProperty ,
                  owl:IrreflexiveProperty .


###  http://purl.org/MMO#Dénote
:Dénote rdf:type owl:ObjectProperty ;
        owl:equivalentProperty [ owl:inverseOf :EstDénotéPar
                               ] ;
        owl:inverseOf :EstDénotéPar ;
        rdf:type owl:AsymmetricProperty ,
                 owl:IrreflexiveProperty ;
        rdfs:domain :Signifiant ;
        rdfs:range :Signifié .


###  http://purl.org/MMO#DénoteObjetGéographique
:DénoteObjetGéographique rdf:type owl:ObjectProperty ;
                         owl:equivalentProperty [ owl:inverseOf :ObjetGéographiqueDénotéPar
                                                ] ;
                         rdfs:subPropertyOf :Dénote ,
                                            [ owl:inverseOf :EstDénotéPar
                                            ] ;
                         owl:inverseOf :ObjetGéographiqueDénotéPar ;
                         rdf:type owl:AsymmetricProperty ,
                                  owl:IrreflexiveProperty ;
                         rdfs:range :Objet_géographique .


###  http://purl.org/MMO#EstComposéDe
:EstComposéDe rdf:type owl:ObjectProperty ;
              owl:equivalentProperty [ owl:inverseOf :Compose
                                     ] ;
              rdf:type owl:AsymmetricProperty ,
                       owl:IrreflexiveProperty .


###  http://purl.org/MMO#EstDénotéPar
:EstDénotéPar rdf:type owl:ObjectProperty ;
              owl:equivalentProperty [ owl:inverseOf :Dénote
                                     ] ;
              rdf:type owl:AsymmetricProperty ,
                       owl:IrreflexiveProperty .


###  http://purl.org/MMO#EstProduitPar
:EstProduitPar rdf:type owl:ObjectProperty ;
               owl:equivalentProperty [ owl:inverseOf :Produit
                                      ] ;
               owl:inverseOf :Produit ;
               rdf:type owl:FunctionalProperty ,
                        owl:AsymmetricProperty ,
                        owl:IrreflexiveProperty .


###  http://purl.org/MMO#EstUtiliséPourProduirePar
:EstUtiliséPourProduirePar rdf:type owl:ObjectProperty ;
                           owl:equivalentProperty [ owl:inverseOf :ProduitAPartirDe
                                                  ] ;
                           owl:inverseOf :ProduitAPartirDe ;
                           rdf:type owl:AsymmetricProperty ,
                                    owl:IrreflexiveProperty ;
                           rdfs:range :Processus .


###  http://purl.org/MMO#IssuDe
:IssuDe rdf:type owl:ObjectProperty ;
        owl:equivalentProperty [ owl:inverseOf :AboutiÀ
                               ] ;
        owl:propertyChainAxiom ( :EstProduitPar
                                 :ProduitAPartirDe
                               ) .


###  http://purl.org/MMO#ObjetGéographiqueDénotéPar
:ObjetGéographiqueDénotéPar rdf:type owl:ObjectProperty ;
                            owl:equivalentProperty [ owl:inverseOf :DénoteObjetGéographique
                                                   ] ;
                            rdfs:subPropertyOf :EstDénotéPar ,
                                               [ owl:inverseOf :Dénote
                                               ] ;
                            rdf:type owl:AsymmetricProperty ,
                                     owl:IrreflexiveProperty .


###  http://purl.org/MMO#Produit
:Produit rdf:type owl:ObjectProperty ;
         owl:equivalentProperty [ owl:inverseOf :EstProduitPar
                                ] ;
         rdf:type owl:InverseFunctionalProperty ,
                  owl:AsymmetricProperty ,
                  owl:IrreflexiveProperty ;
         rdfs:domain :Processus .


###  http://purl.org/MMO#ProduitAPartirDe
:ProduitAPartirDe rdf:type owl:ObjectProperty ;
                  owl:equivalentProperty [ owl:inverseOf :EstUtiliséPourProduirePar
                                         ] ;
                  rdf:type owl:FunctionalProperty ,
                           owl:AsymmetricProperty ,
                           owl:IrreflexiveProperty ;
                  rdfs:domain :Processus .


###  http://purl.org/MMO#Signification_équivalente
:Signification_équivalente rdf:type owl:ObjectProperty ;
                           owl:equivalentProperty [ owl:inverseOf :Signification_équivalente
                                                  ] ;
                           owl:inverseOf :Signification_équivalente ;
                           rdf:type owl:SymmetricProperty ;
                           rdfs:domain :Signifiant ;
                           rdfs:range :Signifiant ;
                           owl:propertyChainAxiom ( :DénoteObjetGéographique
                                                    :ObjetGéographiqueDénotéPar
                                                  ) .


#################################################################
#    Classes
#################################################################

###  http://purl.org/MMO#Carte
:Carte rdf:type owl:Class ;
       owl:equivalentClass [ owl:intersectionOf ( :Production
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :EstProduitPar ;
                                                    owl:someValuesFrom :Cartographie
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :EstProduitPar ;
                                                    owl:allValuesFrom :Cartographie
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ;
       rdfs:subClassOf :Production ;
       owl:disjointWith :Cartographie ,
                        :Externalisation ,
                        :FiguréCartographique ,
                        :ObjetBaseDonnée ,
                        :Processus ,
                        :Segmentation ,
                        :Signifiant ,
                        :Sémantisation ,
                        :UnionFiguréCartographie ,
                        :UnionObjetBaseDonnée ,
                        :UnionSignifiant ;
       owl:disjointUnionOf ( :CarteEuclidienne
                             :CarteMentale
                           ) .


###  http://purl.org/MMO#CarteEuclidienne
:CarteEuclidienne rdf:type owl:Class ;
                  rdfs:subClassOf :Carte ;
                  owl:disjointWith :CarteMentale ,
                                   :CartePerceptive ,
                                   :CarteSensible ,
                                   :Cartographie ,
                                   :Externalisation ,
                                   :FiguréCartographique ,
                                   :ObjetBaseDonnée ,
                                   :Processus ,
                                   :Segmentation ,
                                   :Signifiant ,
                                   :Sémantisation ,
                                   :UnionFiguréCartographie ,
                                   :UnionObjetBaseDonnée ,
                                   :UnionSignifiant ;
                  rdfs:comment """Terme repris des travaux d'Élise Olmédo

NB : à changer. Les cartes \"eulidiennes\" peuvent utiliser une représentation non euclidienne de l'espace""" .


###  http://purl.org/MMO#CarteMentale
:CarteMentale rdf:type owl:Class ;
              rdfs:subClassOf :Carte ;
              owl:disjointWith :Cartographie ,
                               :Externalisation ,
                               :FiguréCartographique ,
                               :ObjetBaseDonnée ,
                               :Processus ,
                               :Segmentation ,
                               :Signifiant ,
                               :Sémantisation ,
                               :UnionFiguréCartographie ,
                               :UnionObjetBaseDonnée ,
                               :UnionSignifiant .


###  http://purl.org/MMO#CartePerceptive
:CartePerceptive rdf:type owl:Class ;
                 rdfs:subClassOf :CarteMentale ;
                 owl:disjointWith :Cartographie ,
                                  :Externalisation ,
                                  :FiguréCartographique ,
                                  :ObjetBaseDonnée ,
                                  :Processus ,
                                  :Segmentation ,
                                  :Signifiant ,
                                  :Sémantisation ,
                                  :UnionFiguréCartographie ,
                                  :UnionObjetBaseDonnée ,
                                  :UnionSignifiant .


###  http://purl.org/MMO#CarteSensible
:CarteSensible rdf:type owl:Class ;
               rdfs:subClassOf :CartePerceptive ;
               owl:disjointWith :Cartographie ,
                                :Externalisation ,
                                :FiguréCartographique ,
                                :ObjetBaseDonnée ,
                                :Processus ,
                                :Segmentation ,
                                :Signifiant ,
                                :Sémantisation ,
                                :UnionFiguréCartographie ,
                                :UnionObjetBaseDonnée ,
                                :UnionSignifiant .


###  http://purl.org/MMO#Cartographie
:Cartographie rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty :Produit ;
                                    owl:someValuesFrom :Carte
                                  ] ;
              rdfs:subClassOf :Processus ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :Produit ;
                                owl:someValuesFrom :Carte
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :Produit ;
                                owl:allValuesFrom :Carte
                              ] ;
              owl:disjointWith :FiguréCartographique ,
                               :ObjetBaseDonnée ,
                               :ObjetSpatial ,
                               :Objet_géographique ,
                               :Production ,
                               :RSE ,
                               :Signifiant ,
                               :UnionFiguréCartographie ,
                               :UnionObjetBaseDonnée ,
                               :UnionSignifiant ,
                               :segmentation_output .


###  http://purl.org/MMO#Externalisation
:Externalisation rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Restriction ;
                                       owl:onProperty :Produit ;
                                       owl:someValuesFrom :RSE
                                     ] ;
                 rdfs:subClassOf :Processus ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :Produit ;
                                   owl:someValuesFrom :RSE
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :ProduitAPartirDe ;
                                   owl:someValuesFrom :RSI
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :Produit ;
                                   owl:allValuesFrom :RSE
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :ProduitAPartirDe ;
                                   owl:allValuesFrom :RSI
                                 ] ;
                 owl:disjointWith :FiguréCartographique ,
                                  :ObjetBaseDonnée ,
                                  :ObjetSpatial ,
                                  :Objet_géographique ,
                                  :Production ,
                                  :RSE ,
                                  :Signifiant ,
                                  :UnionFiguréCartographie ,
                                  :UnionObjetBaseDonnée ,
                                  :UnionSignifiant ,
                                  :segmentation_output .


###  http://purl.org/MMO#FiguréCartographique
:FiguréCartographique rdf:type owl:Class ;
                      rdfs:subClassOf :ObjetSpatial ,
                                      :Signifiant ;
                      owl:disjointWith :ObjetBaseDonnée ,
                                       :Objet_géographique ,
                                       :Processus ,
                                       :Production ,
                                       :RSE ,
                                       :Segmentation ,
                                       :Signifié ,
                                       :Sémantisation ,
                                       :UnionSignifié ,
                                       :segmentation_output ;
                      rdfs:comment """Le nom du concept n'est pas assez discriminant

Pour l'instant ce concept décrit : \"Les figurés cartographiques représentant un objet géographique réel (géom)\"

Ainsi les flêches, légende, etc. ne sont pas des figurés cartographiques, donc le terme n'est pas très bien choisi""" .


###  http://purl.org/MMO#ObjetBaseDonnée
:ObjetBaseDonnée rdf:type owl:Class ;
                 rdfs:subClassOf :ObjetSpatial ,
                                 :Signifiant ;
                 owl:disjointWith :Objet_géographique ,
                                  :Processus ,
                                  :Production ,
                                  :RSE ,
                                  :Segmentation ,
                                  :Signifié ,
                                  :Sémantisation ,
                                  :UnionSignifié ,
                                  :segmentation_output ;
                 rdfs:comment "Nom mal choisi. Il s'agit de tous les objets dans une base de donnée géographique représentant un géon" .


###  http://purl.org/MMO#ObjetSpatial
:ObjetSpatial rdf:type owl:Class ;
              owl:disjointWith :Processus ,
                               :Segmentation ,
                               :Sémantisation ;
              owl:disjointUnionOf ( :FiguréCartographique
                                    :ObjetBaseDonnée
                                    :Objet_géographique
                                  ) ;
              rdfs:comment "Cette classe défini n'importe quel type d'objet pouvant avoir une position. Il peut aussi bien s'agir d'objets imaginaires, que de dessins (qui ont une position dans l'espace de la feuille), de données géographique ou d'objets réels." .


###  http://purl.org/MMO#Objet_géographique
:Objet_géographique rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty :EstProduitPar ;
                                          owl:someValuesFrom :Sémantisation
                                        ] ;
                    rdfs:subClassOf :ObjetSpatial ,
                                    :Production ,
                                    :Signifié ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :EstProduitPar ;
                                      owl:someValuesFrom :Sémantisation
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :EstProduitPar ;
                                      owl:allValuesFrom :Sémantisation
                                    ] ;
                    owl:disjointWith :Processus ,
                                     :Segmentation ,
                                     :Signifiant ,
                                     :Sémantisation ,
                                     :UnionFiguréCartographie ,
                                     :UnionObjetBaseDonnée ,
                                     :UnionSignifiant ;
                    rdfs:comment "En l'état un objet géographique est défini comme le produit de la sémantisation d'un RSE" ,
                                 "Tout objet spatial qui existe dans la réalité. Il est donc distinct des Figurés cartographiques et des Objets Base donneé" .


###  http://purl.org/MMO#Processus
:Processus rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Restriction ;
                                 owl:onProperty :Produit ;
                                 owl:someValuesFrom :Production
                               ] ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty :Produit ;
                             owl:allValuesFrom :Production
                           ] ;
           owl:disjointWith :Production ,
                            :RSE ,
                            :Signifiant ,
                            :UnionFiguréCartographie ,
                            :UnionObjetBaseDonnée ,
                            :UnionSignifiant ,
                            :segmentation_output .


###  http://purl.org/MMO#Production
:Production rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :EstProduitPar ;
                                  owl:someValuesFrom :Processus
                                ] ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :EstProduitPar ;
                              owl:someValuesFrom :Processus
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :EstProduitPar ;
                              owl:allValuesFrom :Processus
                            ] ;
            owl:disjointWith :Segmentation ,
                             :Signifiant ,
                             :Sémantisation ,
                             :UnionFiguréCartographie ,
                             :UnionObjetBaseDonnée ,
                             :UnionSignifiant .


###  http://purl.org/MMO#RSE
:RSE rdf:type owl:Class ;
     owl:equivalentClass [ rdf:type owl:Restriction ;
                           owl:onProperty :EstProduitPar ;
                           owl:someValuesFrom :Externalisation
                         ] ;
     rdfs:subClassOf :Production ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :EstProduitPar ;
                       owl:someValuesFrom :Externalisation
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :EstUtiliséPourProduirePar ;
                       owl:someValuesFrom :Segmentation
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :EstProduitPar ;
                       owl:allValuesFrom :Externalisation
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :EstUtiliséPourProduirePar ;
                       owl:allValuesFrom :Segmentation
                     ] ;
     owl:disjointWith :Segmentation ,
                      :Signifiant ,
                      :Sémantisation ,
                      :UnionFiguréCartographie ,
                      :UnionObjetBaseDonnée ,
                      :UnionSignifiant .


###  http://purl.org/MMO#RSI
:RSI rdf:type owl:Class .


###  http://purl.org/MMO#Segmentation
:Segmentation rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty :Produit ;
                                    owl:someValuesFrom :segmentation_output
                                  ] ;
              rdfs:subClassOf :Processus ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :Produit ;
                                owl:someValuesFrom :segmentation_output
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :ProduitAPartirDe ;
                                owl:someValuesFrom :RSE
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :Produit ;
                                owl:allValuesFrom :segmentation_output
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :ProduitAPartirDe ;
                                owl:allValuesFrom :RSE
                              ] ;
              owl:disjointWith :Signifiant ,
                               :UnionFiguréCartographie ,
                               :UnionObjetBaseDonnée ,
                               :UnionSignifiant ,
                               :segmentation_output .


###  http://purl.org/MMO#Signifiant
:Signifiant rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :Dénote ;
                                  owl:someValuesFrom :Signifié
                                ] ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :Dénote ;
                              owl:allValuesFrom :Signifié
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :Dénote ;
                              owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :Signifié
                            ] ;
            owl:disjointWith :Signifié ,
                             :Sémantisation ,
                             :UnionSignifié ,
                             :segmentation_output .


###  http://purl.org/MMO#Signifié
:Signifié rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :EstDénotéPar ;
                                owl:someValuesFrom :Signifiant
                              ] ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :EstDénotéPar ;
                            owl:someValuesFrom :Signifiant
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :EstDénotéPar ;
                            owl:allValuesFrom :Signifiant
                          ] ;
          owl:disjointWith :UnionFiguréCartographie ,
                           :UnionObjetBaseDonnée ,
                           :UnionSignifiant .


###  http://purl.org/MMO#Sémantisation
:Sémantisation rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty :Produit ;
                                     owl:someValuesFrom :Objet_géographique
                                   ] ;
               rdfs:subClassOf :Processus ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :Produit ;
                                 owl:someValuesFrom :Objet_géographique
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :ProduitAPartirDe ;
                                 owl:someValuesFrom :segmentation_output
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :Produit ;
                                 owl:allValuesFrom :Objet_géographique
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :ProduitAPartirDe ;
                                 owl:allValuesFrom :segmentation_output
                               ] ;
               owl:disjointWith :UnionFiguréCartographie ,
                                :UnionObjetBaseDonnée ,
                                :UnionSignifiant ,
                                :segmentation_output .


###  http://purl.org/MMO#Union
:Union rdf:type owl:Class ;
       owl:equivalentClass [ rdf:type owl:Restriction ;
                             owl:onProperty :EstComposéDe ;
                             owl:minCardinality "2"^^xsd:nonNegativeInteger
                           ] .


###  http://purl.org/MMO#UnionFiguréCartographie
:UnionFiguréCartographie rdf:type owl:Class ;
                         owl:equivalentClass [ owl:intersectionOf ( :UnionSignifiant
                                                                    [ rdf:type owl:Restriction ;
                                                                      owl:onProperty :EstComposéDe ;
                                                                      owl:someValuesFrom :FiguréCartographique
                                                                    ]
                                                                  ) ;
                                               rdf:type owl:Class
                                             ] ;
                         rdfs:subClassOf :UnionSignifiant ;
                         owl:disjointWith :UnionObjetBaseDonnée ,
                                          :UnionSignifié ,
                                          :segmentation_output .


###  http://purl.org/MMO#UnionObjetBaseDonnée
:UnionObjetBaseDonnée rdf:type owl:Class ;
                      owl:equivalentClass [ owl:intersectionOf ( :UnionSignifiant
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty :EstComposéDe ;
                                                                   owl:someValuesFrom :ObjetBaseDonnée
                                                                 ]
                                                               ) ;
                                            rdf:type owl:Class
                                          ] ;
                      rdfs:subClassOf :UnionSignifiant ;
                      owl:disjointWith :UnionSignifié ,
                                       :segmentation_output .


###  http://purl.org/MMO#UnionSignifiant
:UnionSignifiant rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Restriction ;
